    #end
    </ul>

    #set($cnt = 1)
     #if (!$txtNomEtiqueta.getSiblings().isEmpty())
	#foreach ($cur_txtNomEtiqueta in $txtNomEtiqueta.getSiblings())
  <div id="tabs-$cnt">
    
